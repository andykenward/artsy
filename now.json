{
  "version": 2,
  "name": "ori-homework-artsy",
  "alias": "ori-homework-artsy.andykenward.com",
  "scope": "andykenward",
  "regions": ["lhr1"],
  "github": {
    "enabled": false,
    "autoAlias": false
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
